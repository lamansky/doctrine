{
  "name": "lamansky/doctrine",
  "version": "1.0.1",
  "description": "A collection of classes useful in almost any project that uses the Doctrine ORM.",
  "type": "library",
  "keywords": [
    "Doctrine2",
    "Doctrine type"
  ],
  "homepage": "https://github.com/lamansky/doctrine",
  "license": "MIT",
  "authors": [
    {
      "name": "Fr. John Lamansky",
      "homepage": "https://lamansky.com"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "doctrine/annotations": "^1.13",
    "doctrine/orm": "^2.11.2",
    "nesbot/carbon": "^2.57.0",
    "symfony/cache": "^5.4"
  },
  "require-dev": {
    "lamansky/phpcs": "^1"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {"Lamansky\\Doctrine\\": "src/"}
  }
}
